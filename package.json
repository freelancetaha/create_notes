{
  "pricing": "Trial",
  "name": "sparkclub",
  "displayName": "Spark Club",
  "description": "A note-taking and task management extension for programmers!",
  "version": "0.0.6",
  "publisher": "taha-ale",
  "engines": {
    "vscode": "^1.96.0"
  },
  "sponsor": {
    "url": "https://github.com/freelancetaha/sparkclub"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/freelancetaha/create_notes"
  },
  "activationEvents": [
    "onCommand:notes.createnote",
    "onCommand:notes.viewnotes",
    "onCommand:notes.clearnotes",
    "onCommand:notes.addname"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "notes.createNote",
        "title": "Create Note"
      },
      {
        "command": "notes.viewNotes",
        "title": "View Notes"
      },
      {
        "command": "notes.clearNotes",
        "title": "Clear All Notes"
      },
      {
        "command": "notes.addName",
        "title": "Add Your Name"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.96.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.16.0"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "fast-xml-parser": "^4.5.1",
    "vsce": "^1.103.1",
    "vscode-nls": "^5.2.0"
  },
  "icon": "assets/logo.png",
  "categories": [
    "Programming Languages"
  ]
}
